---
import YouTube from '../assets/icons/youtube.svg';

interface Props {
	iconSize: number;
}

const { iconSize } = Astro.props;
const link = "https://music.youtube.com/playlist?list=PL2Fq-K0QdOQiJpufsnhEd1z3xOv2JMHuk";

---

<div>
	<a href={link} target="_blank" rel="noopener noreferrer">
		<YouTube width={iconSize} height={iconSize} fill="#FF0000" class="opacity-70" />
	</a>
</div>